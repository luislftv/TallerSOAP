@model ClienteSOAP.ServiceParticipante.participante

@{
    ViewBag.Title = "BuscarParticipante";

}

<h2>BuscarParticipante</h2>
<br>
<br>

<div class="row">

    <div class="col-md-3">
        <form action="/Participante/BuscarParticipante" method="post">
            <input type="text" name="id" class="form-control" placeholder="ID" aria-label="First name"><br><br>

            <input type="text" name="apodo" class="form-control" placeholder="Apodo" aria-label="Last name"><br><br>

            <input type="datetime-local" name="fechaIns"><br><br>

            <input type="datetime-local" name="fechaCad"><br><br>

            <button type="submit" name="btnConfirmar" class="btn btn-primary">Confirmar</button>

            
        </form>
    </div>

</div>
